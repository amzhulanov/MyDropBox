<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.geekbrains.client.RegUserController">
    <children>
        <GridPane layoutX="14.0" layoutY="14.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>

                <Label text="Name"/>
                <Label text="Login" GridPane.rowIndex="1"/>
                <Label text="Password" GridPane.rowIndex="2"/>
                <Label layoutX="10.0" layoutY="73.0" text="Repeat password" GridPane.rowIndex="3"/>
                <TextField fx:id="name" promptText="name" GridPane.columnIndex="1"/>
                <TextField fx:id="login" promptText="login" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <PasswordField fx:id="password" promptText="password" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <PasswordField fx:id="passwordRepeat" layoutX="110.0" layoutY="73.0" promptText="repeat password" GridPane.columnIndex="1"
                               GridPane.rowIndex="3"/>
                <Button fx:id="register" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Register"
                        GridPane.columnIndex="2"/>
                <Button fx:id="cancel" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Cancel"
                        GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
